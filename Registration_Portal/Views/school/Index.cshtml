@model IEnumerable<Registration_Portal.Models.school>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.school_name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Xth_marks)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.XIIth_marks)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.school_name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Xth_marks)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.XIIth_marks)
        </td>
